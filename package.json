{
  "name": "authbot",
  "version": "0.0.1",
  "description": "A service to enable web auth and login to a custom domain facilitated via a Telegram bot.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --cache src",
    "development": "NODE_ENV=development node --harmony-async-await index.js",
    "test": "NODE_ENV=test node --harmony-async-await index.js",
    "tmp": "NODE_ENV=development node --harmony-async-await tmp/index.js",
    "start": "node --harmony-async-await index.js",
    "pm2start": "pm2 start index.js --node-args='--harmony-async-await' --name `basename $PWD`"
  },
  "author": "Evan Summers",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.6",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-router": "^7.0.1",
    "lodash": "^4.17.2",
    "node-fetch": "^1.6.3",
    "redis": "^2.6.3",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^3.12.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evanx/authbot.git"
  },
  "bugs": {
    "url": "https://github.com/evanx/authbot/issues"
  },
  "homepage": "https://github.com/evanx/authbot#readme"
